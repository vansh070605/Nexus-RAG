{
    "version": 2,
    "name": "nexus-rag",
    "builds": [
        {
            "src": "frontend/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/frontend/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/index.html"
        }
    ]
}